{
    "extends": "airbnb-base",
    "parserOptions": {
        "sourceType": "script"
    },
    "env": {
        "mocha": true
    },
    "rules": {
    "no-unused-vars": ["error", { "varsIgnorePattern": "log" }],
    "no-underscore-dangle": "off",
    "newline-per-chained-call": "off",
    "no-nested-ternary": "off"
    }
}